{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.2.1",
    "generators": {
      "APIv1": {
        "generatorName": "typescript-angular",
        "output": "src/app/api/v1/",
        "inputSpec": "https://staging.kitos.dk/swagger/docs/1",
        "additionalProperties": {
          "ngVersion": "15",
          "supportsES6": "true",
          "withInterfaces": true
        }
      },
      "APIv2": {
        "generatorName": "typescript-angular",
        "output": "src/app/api/v2/",
        "inputSpec": "https://staging.kitos.dk/swagger/docs/2",
        "additionalProperties": {
          "ngVersion": "15",
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
